{  
  "metadata": [
    {
        "src": [
            {
                "src": ".",
                "files": [
                    "src/*.cs"
                ]
            }
        ],        
        "globalNamespaceId": "Global",
        "dest": "api"
    }
],
  "build": {
    "content": [
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md",
          "api/**.md",
          "api/**/toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "../microsoftfx"
    ],
    "postProcessors": [
      "ExtractSearchIndex"
    ],
    "globalMetadata": {
      "_appFooter": "<span>Copyright Â© AndrewMJordan<br>Generated with <strong>DocFX</strong></span>",
      "_appTitle": "MicrosoftFX",
      "_enableSearch": "true",
      "_gitContribute": {
        "repo": "https://github.com/AndrewMJordan/MicrosoftFX"
      },
      "_gitUrlPattern": "github"
    },
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  },
  "sitemap":
  {
    "baseUrl": "https://andrewmjordan.github.io/MicrosoftFX"
  }
}
