{
  "metadata": [
      {
          "src": [
              {
                  "src": ".",
                  "files": [
                      "src/*.cs"
                  ]
              }
          ],
          "globalNamespaceId": "Global",
          "dest": "api"
      }
  ],
  "build": {
      "globalMetadata": {
          "_appTitle": "MicrosoftFX",
          "_appFooter": "MicrosoftFX",
          "_enableSearch": true
      },
      "content": [
          {
              "files": [
                  "toc.yml",
                  "index.md"
              ]
          },
          {
              "src": "api",
              "files": [
                  "*.yml"
              ],
              "dest": "api"
          },
          {
              "src": "articles",
              "files": [
                  "toc.yml",
                  "*.md"
              ],
              "dest": "articles"
          }
      ],
      "sitemap": {
          "baseUrl": "https://andrewmjordan.github.io/MicrosoftFX",
          "changefreq": "weekly",
          "fileOptions": {
              "api/*": {
                  "changefreq": "daily"
              }
          }
      },
      "dest": "_site"
  }
}